1- def bubble_sort(arr):
    n = len(arr)
    # Percorre todos os elementos do array
    for i in range(n):
        # Últimos i elementos já estão no lugar certo
        for j in range(0, n-i-1):
            # Percorre o array de 0 até n-i-1
            # Troca se o elemento encontrado for maior que o próximo
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]

# Exemplo de uso
arr = [64, 34, 25, 12, 22, 11, 90]
bubble_sort(arr)
print("Array ordenado:")
for i in range(len(arr)):
    print("%d" % arr[i]),



2- def imprimir_vogais(frase):
    vogais = "aeiouAEIOU"
    vogais_na_frase = [letra for letra in frase if letra in vogais]
    print("Vogais na frase:", ''.join(vogais_na_frase))

frase = input("Digite uma palavra ou frase: ")
imprimir_vogais(frase)
